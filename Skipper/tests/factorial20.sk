// Recursive factorial of 20.
// NOTE: int is 32-bit in the current VM, so the result will overflow.
fn fact(int n) -> int {
    if (n <= 1) {
        return 1;
    }
    return n * fact(n - 1);
}

fn main() -> int {
    int result = fact(20);
    print("fact(20) result:");
    print(result);
    return result;
}

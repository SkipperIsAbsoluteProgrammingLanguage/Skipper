// Allocates many arrays to trigger GC cycles.
fn main() -> int {
    int i = 0;
    int[] keep = new int[1024];

    while (i < 4000) {
        int[] temp = new int[1024];
        temp[0] = i;
        if (i % 100 == 0) {
            keep = temp;
        }
        i = i + 1;
    }

    print("gc demo done, keep[0]:");
    print(keep[0]);
    return 0;
}
